  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SoloSculptor/SoloSync/issues"
  },
  "homepage": "https://github.com/SoloSculptor/SoloSync#readme",
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5"
  },
  "dependencies": {
    "chokidar": "^2.1.2",
    "config": "^3.0.1",
    "request": "^2.88.0"
